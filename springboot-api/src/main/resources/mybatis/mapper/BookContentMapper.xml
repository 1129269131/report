<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace= "com.tjh.springbootapi.mapper.BookContentMapper">

    <!-- 通用查询映射结果-->
    <resultMap id= "BaseResultMap" type="com.tjh.springbootapi.entity.BookContent">
        <id column="id" property="id"/>
        <result column="book_id" property="bookId"/>
        <result column="index_id" property="indexId"/>
        <result column="index_num" property="indexNum"/>
        <result column="content" property="content"/>
    </resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, book_id, index_id, index_num, content
    </sql>

</mapper>
